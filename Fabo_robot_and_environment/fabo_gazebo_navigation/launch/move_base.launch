<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
    <rosparam file="$(find fabo_gazebo_navigation)/config/fabo/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find fabo_gazebo_navigation)/config/fabo/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find fabo_gazebo_navigation)/config/fabo/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find fabo_gazebo_navigation)/config/fabo/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find fabo_gazebo_navigation)/config/fabo/base_local_planner_params4.yaml" command="load" />
    <rosparam file="$(find fabo_gazebo_navigation)/config/fabo/global_planner_params.yaml" command="load" />
  </node>
  
</launch>
