<launch>



    <!-- 运行地图服务器，并且加载设置的地图-->
    <node name="map_server" pkg="map_server" type="map_server" args="/home/zhjd/fabo_gazebo/src/fabo_moveit_gazebo/fabo_gazebo_navigation/maps/map.yaml"/>

    <!-- 运行move_base节点 -->
    <include file="$(find fabo_gazebo_navigation)/launch/fake_move_base.launch"/>

    <!-- 运行虚拟定位，兼容AMCL输出 -->
    <node pkg="fake_localization" type="fake_localization" name="fake_localization" output="screen" />

    <!-- 对于虚拟定位，需要设置一个/odom与/map之间的静态坐标变换 -->
    <node pkg="tf" type="static_transform_publisher" name="map_odom_broadcaster" args="0 0 0 0 0 0 /map /odom 100" />

    <!-- 运行rviz -->
    <!-- <node pkg="rviz" type="rviz" name="rviz" args="-d $(find fabo_gazebo_navigation)/rviz/nav.rviz"/> -->

</launch>
